    <contexts>
      
      <context attribute="Normal Text" lineEndContext="#stay" name="Normal">
        <keyword attribute="Keyword" context="#stay" String="keywords"/>
        <Float attribute="Number" context="#stay"/>
	    <Int attribute="Number" context="#stay"/>
        <DetectChar attribute="String" context="String single" char="&apos;" />
        <DetectChar attribute="String" context="String double" char="&quot;" />
        <Detect2Chars attribute="Comment" context="Comment multiline" char="/" char1="*"/>
        <Detect2Chars attribute="Comment" context="Comment line" char="/" char1="/"/>
      </context>
      
      <context attribute="String" lineEndContext="#pop" name="String single">
        <DetectChar attribute="String" context="#pop" char="&apos;"/>
      </context>
      
      <context attribute="String" lineEndContext="#pop" name="String double">
        <DetectChar attribute="String" context="#pop" char="&quot;"/>
      </context>
      
      <context attribute="Comment" lineEndContext="#stay" name="Comment multiline">
        <DetectSpaces />
        <Detect2Chars attribute="Comment" context="#pop" char="*" char1="/"/>
      </context>
      
      <context attribute="Comment" lineEndContext="#pop" name="Comment line">
        <DetectSpaces />
      </context>
    </contexts>
    
